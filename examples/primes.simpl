((fun (main)
   (vars [(i 2) (j 0) (acc 0)]
         (while (<= i 1000)
                (set j 2)
                (set acc 0)
                (while (< j i) 
                       (iif (= (mod i j) 0) 
                            (set acc 1) 
                            (skip)) 
                       (set j (+ j 1)))
                (iif (= acc 0) 
                     (seq (print i) (print " ")) 
                     (skip))
                (set i (+ i 1)))
         (return 0))))